## Bom file will override the halyard version in .hal/config
## And over write the respective service images in service-settings, If service is not defined here it won't consider to over written

version: 1.30.1
services:
  kayenta:
    image: quay.io/opsmxpublic/ubi8-spin-kayenta:ecbe3f6-5
  fiat:
    image: quay.io/opsmxpublic/ubi8-oes-fiat:dcd7dab-7
  clouddriver: 
    image: quay.io/opsmxpublic/ubi8-oes-clouddriver:31a3bbdbd
  front50:
    image: quay.io/opsmxpublic/ubi8-oes-front50:30308b42-11
  echo:
    image: quay.io/opsmxpublic/ubi8-oes-echo:8fd3e1d6-7
  rosco:
    image: quay.io/opsmxpublic/ubi8-spin-rosco:4bd1f58-5
  igor:
    image: quay.io/opsmxpublic/ubi8-spin-igor:b42e23d-7
  deck:
    image: quay.io/opsmxpublic/ubi8-oes-deck:6f60379a8-8
  orca:
    image: quay.io/opsmxpublic/ubi8-oes-orca:cb2cc40d7-18
  gate:
    image: quay.io/opsmxpublic/ubi8-oes-gate:80130c7d-11
